    <!-- MAIN CONTENT -->

    <main>
      <!-- ABOUT INTRO -->

      <section id="intro">
        <section class="slogan">
          <article>
            <h1>About Us</h1>
            <h4>
              Bright Event Organizers was founded in October 2015 with a clear
              mission: to organize top-notch events that leave our clients
              completely satisfied. Our goal is to cultivate lasting and
              mutually beneficial relationships with our clients by offering
              innovative solutions tailored to their specific needs..
            </h4>
          </article>
          <ul class="intro_cta">
            <li class="contact">
              <a href="/contact">Contact Us</a>
            </li>
            <li class="our_project">
              <a href="/portfolio">Our Projects</a>
            </li>
          </ul>
        </section>
        <section id="image">
          <img src="/Images/home.png" alt="" />
        </section>
      </section>

      <!-- MISSION -->

      <section id="mission" class="about-column">
        <img
          src="https://images.unsplash.com/photo-1521316730702-829a8e30dfd0?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MTB8fG5nbyUyMG1pc3Npb24lMjBpbWFnZXxlbnwwfHwwfHx8MA%3D%3D"
          alt=""
          class="column-image"
        />
        <article>
          <h2><img src="/Images/Rocket.png" /> Our Mission</h2>
          <p>
            Providing professional and creative event planning solutions for
            clients.
          </p>
        </article>
      </section>

      <!-- VISION -->

      <section id="vision" class="about-column">
        <article>
          <h2><img src="/Images/Eye.png" alt="" /> Our Vision</h2>
          <p>Excel internationally in managing local and global events.</p>
        </article>
        <img
          src="https://images.unsplash.com/photo-1488190211105-8b0e65b80b4e?w=600&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8NHx8dmlzaW9uJTIwaW1hZ2V8ZW58MHx8MHx8fDA%3D"
          alt=""
          class="column-image"
        />
      </section>

      <!-- CORE VALUES -->
      <section id="values" class="about-column">
        <figure class="values">
          <img
